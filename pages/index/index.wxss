/* index */
page{
  width: 100%;
  height: 100%;
  position: relative;
  left: 0;
  top: 0;
}
.bgc{
  width: 750rpx;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
}
.logo{
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
}
.content{
  width: 750rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  color: #fff;
}
.name{
  margin-left: 50rpx;
  font-size: 48rpx;

}
.top{
  padding: 50rpx 50rpx;
  display: flex;
  align-items: center;
}
.info{
  padding: 0 50rpx;
  height: 70rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 30rpx;
  /* margin: 30rpx 0; */
  box-sizing: border-box;
}
.info van-icon{
  width: 60rpx;
}
.user-title{
  display: flex;
  align-items: center;
}
.user-title van-icon{
  width: 50rpx;
  display: flex;
  align-items: center;
}
.enter{
  width: 650rpx;
  margin: 40rpx auto;
}
.enter button{
  text-align: right;
  width: 400rpx;
  background-color: #fff;
  line-height: 1.5;
}
.enter button::after{
  height: 100%;
  border: 0;
}
.enter .van-cell{
  border-radius: 20rpx;
}
.store{
  color: #333;
  margin: 0 50rpx;
}
.store-title{
  margin-bottom: 20rpx;
}
.pics{
  overflow-x: scroll;
  white-space: nowrap;
}
.store-pic{
  width: 310rpx;
  height: 310rpx;
  border-radius: 40rpx;
  margin-right: 30rpx;
}
.store-pic:last-of-type{
  margin-right: 0;
}